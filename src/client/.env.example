# Client Environment Variables
# These variables are prefixed with VITE_ to be accessible in the browser

# Node Environment
VITE_NODE_ENV=development

# API Configuration
VITE_API_URL=http://localhost:3001/api
VITE_CLIENT_URL=http://localhost:3000

# App Configuration
VITE_APP_NAME=DivvyUp
VITE_APP_VERSION=1.0.0
VITE_ENABLE_DEBUG=false

# Payment Provider Configuration
VITE_PAYMENT_PROVIDER=none

# Stripe Configuration (if using Stripe)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Venmo Configuration (if using Venmo)
VITE_VENMO_CLIENT_ID=your-venmo-client-id

# PayPal Configuration (if using PayPal)
VITE_PAYPAL_CLIENT_ID=your-paypal-client-id
